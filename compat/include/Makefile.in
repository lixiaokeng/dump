top_srcdir=	@top_srcdir@
srcdir=		@srcdir@

@MCONFIG@

all::		pathnames.h

pathnames.h:	pathnames.h.in
	sed -e "s|__DUMPDATES__|$(DUMPDATESPATH)|g" $< > $@

install::

clean::
	$(RM) -f \#* *~ core pathnames.h

distclean::	clean
	$(RM) -f Makefile Makefile.old .depend

